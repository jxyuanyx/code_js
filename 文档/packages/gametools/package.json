{
  "name": "gametools",
  "version": "0.0.1",
  "description": "The package template for getting started.",
  "author": "Cocos Creator",
  "main": "main.js",
  "main-menu": {
    "游戏辅助工具/proto转js": {
      "message": "gametools:openProtoTool"
    },
    "游戏辅助工具/版本分包": {
      "message": "gametools:openPackageTool"
    },
    "游戏辅助工具/构建后自动加密": {
      "message": "gametools:openAutoEncrypt"
    }
  },
  "panels": {
    "protoPanel": {
      "main": "panel/protoTool.js",
      "type": "dockable",
      "title": "proto转js辅助工具",
      "width": 400,
      "height": 800
    },
    "packagePanel": {
      "main": "panel/packageTool.js",
      "type": "dockable",
      "title": "分包工具",
      "width": 400,
      "height": 800
    },
    "autoEncryptPanel": {
      "main": "panel/encrypt.js",
      "type": "dockable",
      "title": "自动加密",
      "width": 400,
      "height": 800
    }
  },
  "dependencies": {
    "ant-design-vue": "^1.7.4",
    "archiver": "^5.3.0",
    "compressing": "^1.5.1",
    "fs-extra": "^9.1.0",
    "ftp": "^0.3.10",
    "gulp": "^4.0.2",
    "gulp-cache": "^1.1.3",
    "gulp-imagemin": "^7.1.0",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2"
  }
}
